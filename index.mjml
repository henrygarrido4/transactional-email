          />
        </mj-column>
        <mj-column>
          <mj-image
            align="center"
            padding="0px"
            width="42px"
            src="https://exclusivecutz.com/onnit/google.png"
            href="https://www.googleplus.com/onnit/"
            alt="google icon"
          />
        </mj-column>
        <mj-column>
          <mj-image
            align="center"
            padding="0px"
            width="42px"
            src="https://exclusivecutz.com/onnit/yt.png"
            href="https://www.youtube.com/onnit/"
            alt="youtube icon"
          />
        </mj-column>
      </mj-group>
    </mj-section>
  </mj-body>
</mjml>
